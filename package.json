{
  "name": "tevet-troc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && npx typeorm -d dist/data-source.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "create:lib": "node utils/create-lib.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "cli-table3": "^0.6.5",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "string-width": "^7.2.0",
    "tevet-troc": "link:"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@fastify/static": "^8.1.1",
    "@fastify/view": "^10.0.2",
    "@flydotio/dockerfile": "^0.7.8",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.1",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/testing": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@sinclair/typebox": "^0.34.30",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "nanoid": "^5.1.3",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/ambulance(|/.*)$": "<rootDir>/libs/buss/ambulance/src/$1",
      "^@app/buss/emergency(|/.*)$": "<rootDir>/libs/buss/emergency/src/$1",
      "^@app/dashboard(|/.*)$": "<rootDir>/libs/dashboard/src/$1",
      "^@app/emergency(|/.*)$": "<rootDir>/libs/emergency/src/$1",
      "^@app/hospital(|/.*)$": "<rootDir>/libs/hospital/src/$1",
      "^@app/http-response(|/.*)$": "<rootDir>/libs/http-response/src/$1",
      "^@app/models(|/.*)$": "<rootDir>/libs/models/src/$1",
      "^@app/test(|/.*)$": "<rootDir>/libs/test/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1"
    }
  }
}