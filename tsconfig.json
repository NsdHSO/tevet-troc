{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "types": [
      "jest",
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/ambulance": [
        "libs/buss/ambulance/src"
      ],
      "@app/ambulance/*": [
        "libs/buss/ambulance/src/*"
      ],
      "@app/dashboard": [
        "libs/buss/dashboard/src"
      ],
      "@app/dashboard/*": [
        "libs/buss/dashboard/src/*"
      ],
      "@app/hospital": [
        "libs/buss/hospital/src"
      ],
      "@app/hospital/*": [
        "libs/buss/hospital/src/*"
      ],
      "@app/http-response": [
        "libs/http-response/src"
      ],
      "@app/http-response/*": [
        "libs/http-response/src/*"
      ],
      "@app/models": [
        "libs/models/src"
      ],
      "@app/models/*": [
        "libs/models/src/*"
      ],
      "@app/utils": [
        "libs/utils/src"
      ],
      "@app/utils/*": [
        "libs/utils/src/*"
      ]
    }
  }
}
